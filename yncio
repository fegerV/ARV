Indexes:
    "ar_content_pkey" PRIMARY KEY, btree (id)
    "ar_content_unique_id_key" UNIQUE CONSTRAINT, btree (unique_id)
    "ix_ar_content_company_id" btree (company_id)
    "ix_ar_content_id" btree (id)
    "ix_ar_content_project_id" btree (project_id)
    "ix_ar_content_project_order" UNIQUE, btree (project_id, order_number)
Check constraints:
    "check_duration_years" CHECK (duration_years = ANY (ARRAY[1, 3, 5]))
    "check_views_count_non_negative" CHECK (views_count >= 0)
